# Gemfile

source "https://rubygems.org"

# Specify the Jekyll version
gem "jekyll", "~> 4.4.1"

# Required for Windows environments
gem 'wdm', '>= 0.1.0' if Gem.win_platform?

# Webrick is needed for serving the site
gem "webrick", "~> 1.7"

# Sitemap plugin
gem 'jekyll-sitemap'

# SEO tag plugin
gem "jekyll-seo-tag"
